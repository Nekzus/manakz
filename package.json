{
  "name": "manakz",
  "version": "0.0.0-development",
  "description": "Console Project Helper",
  "main": "./bin/openpj.js",
  "engines": {
    "node": ">=15"
  },
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "manakz": "./bin/openpj.js"
  },
  "scripts": {
    "start": "node ./bin/openpj.js",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main",
    "dev": "node test/index.js",
    "lint:fix": "eslint --fix ./*.js"
  },
  "keywords": [
    "open project",
    "Visual Studio Code",
    "manage dependencies",
    "development server",
    "project setup",
    "streamlined workflow",
    "project assistant",
    "project toolkit",
    "command-line tool",
    "project automation",
    "project configuration",
    "development environment"
  ],
  "author": "nekzus",
  "license": "MIT",
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "prompts": "^2.4.2",
    "yargs": "^17.7.2"
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-commonjs": "^1.0.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "semantic-release": "^22.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nekzus/manakz.git"
  },
  "bugs": {
    "url": "https://github.com/Nekzus/manakz/issues"
  },
  "homepage": "https://github.com/Nekzus/manakz#readme"
